{
    "compilerOptions": {
        "target": "es2017",
        "jsx": "react",
        "module": "commonjs",
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "sourceMap": true,
        "removeComments": true,
        "noImplicitAny": false,
        "downlevelIteration": true,
        "forceConsistentCasingInFileNames": true,
        "esModuleInterop": true,
        "baseUrl": "./",
        
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
}